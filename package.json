{
  "name": "node-version",
  "version": "0.0.1-snapshot",
  "description": "A command line node module to deal with \"bumping\" and \"npm version\"",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "npm-shrinkwrap.json"
  ],
  "scripts": {
    "build-es5": "npm run delete lib-es5 && node ./node_modules/babel-cli/bin/babel.js lib --presets es2015 --out-dir lib-es5",
    "delete": "node node_modules/rimraf/bin.js",
    "start": "node ./lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bump-major": "npm version major -m \"Upgrade to major version: %s\"",
    "bump-minor": "npm version minor -m \"Upgrade to minor version: %s\"",
    "bump-patch": "npm version patch -m \"Upgrade to version: %s\"",
    "version": "npm run build-es5 && git add -A lib-es5",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rochejul/node-version.git"
  },
  "keywords": [
    "bump",
    "npm",
    "version",
    "cli"
  ],
  "author": "Julien Roche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rochejul/node-version/issues"
  },
  "homepage": "https://github.com/rochejul/node-version#readme",
  "dependencies": {
    "json5": "0.5.0",
    "minimist": "1.2.0",
    "semver": "5.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.4.0",
    "rimraf": "2.5.2"
  },
  "bin": {
    "version": "./bin/version"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  }
}
